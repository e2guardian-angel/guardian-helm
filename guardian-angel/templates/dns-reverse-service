apiVersion: v1
kind: Service
metadata:
  name: dns-reverse
  labels:
    app: dns-reverse
spec:
  type: ClusterIP
  selector:
    app: dns-reverse
  ports:
    - name: dns-tcp
      protocol: TCP
      port: {{ .Values.reverseDnsPort }}
      targetPort: 9053
    - name: dns-udp
      protocol: UDP
      port: {{ .Values.reverseDnsPort }}
      targetPort: 9053
